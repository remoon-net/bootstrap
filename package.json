{
	"name": "@remoon.net/bootstrap",
	"version": "0.4.1",
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/remoon-net/bootstrap.git"
	},
	"bugs": {
		"url": "https://github.com/remoon-net/bootstrap/issues"
	},
	"homepage": "https://github.com/remoon-net/bootstrap#readme",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check ."
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*",
		"faker.js"
	],
	"sideEffects": [
		"**/*.css"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"peerDependencies": {
		"@remoon.net/bootstrap-fork": "*",
		"svelte": "^5.0.0"
	},
	"devDependencies": {
		"@popperjs/core": "^2.11.8",
		"@remoon.net/bootstrap-fork": "^5.3.8",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.12.0",
		"@sveltejs/package": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@types/bootstrap": "^5.2.10",
		"@types/node": "^22.13.10",
		"bootstrap-icons": "^1.11.3",
		"prettier": "^3.3.2",
		"prettier-plugin-svelte": "^3.2.6",
		"publint": "^0.2.0",
		"sass-embedded": "^1.83.0",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"vite": "^5.0.11"
	},
	"dependencies": {
		"uuid": "^11.0.5"
	},
	"packageManager": "pnpm@9.11.0+sha512.0a203ffaed5a3f63242cd064c8fb5892366c103e328079318f78062f24ea8c9d50bc6a47aa3567cabefd824d170e78fa2745ed1f16b132e16436146b7688f19b"
}
